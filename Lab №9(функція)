#include <cstdlib>
#include <iostream>
#include <string.h>
#include <math.h>
using namespace std;

double a = 3.18;
double x = 2.64;
double y = 3.27;
double z = 2.64;

double p(double k) 
{
return tan(a * (k)) + sin((k));
}

int main(int argc, char* argv[])
{
double F = sin(p(x)) / sqrt(x + p(y)) + p(z) * p(z) / 2 * x - y * log10(x);
cout<<F<<endl;

system("PAUSE");
return EXIT_SUCCESS;
}
